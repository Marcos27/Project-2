<h1>Add a new SONG!</h1>

<%= form_for @musics do |f| %>

    <div>
        <%= f.label :artist %>
        <%= f.text_field :artist %>
    </div>

    <div>
        <%= f.label :song %>
        <%= f.text_field :song %>
    </div>

    <div>
        <%= f.label :genre %>
        <%= f.text_field :genre %>
    </div>


    <%= f.submit "Add new bean!" %>

<% end %>

<%= link_to "Back to the jam list!", musics_path %>
