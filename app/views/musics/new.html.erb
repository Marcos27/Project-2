<h1>Add a new Song!</h1>

<%= form_for @music do |f| %>

    <div>
        <%= f.label :song %>
        <%= f.text_field :song %>
    </div>

    <div>
        <%= f.label :artist %>
        <%= f.text_field :artist %>
    </div>

    <div>
        <%= f.label :genre %>
        <%= f.text_field :genre %>
    </div>



    <%= f.submit "Add new Song!" %>

<% end %>

<%= link_to "Back to the Play-list!", musics_path %>
